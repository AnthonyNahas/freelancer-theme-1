{
  "name": "freelancer-theme",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "build:doc": "cd ../ && gulp build:doc",
    "build:prod": "ng build --prod ",
    "build:client-and-server-bundles": "ng build --prod && ng run freelancer-theme:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate:prerender": "cd dist && node prerender",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "npm run prettier -s",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:prod": "ng serve --prod",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "dependencies": {
    "@angular/animations": "^7.2.1",
    "@angular/cdk": "^7.0.1",
    "@angular/common": "^7.2.1",
    "@angular/compiler": "^7.2.1",
    "@angular/core": "^7.2.1",
    "@angular/flex-layout": "7.0.0-beta.23",
    "@angular/forms": "^7.2.1",
    "@angular/http": "^7.2.1",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "^7.2.1",
    "@angular/platform-browser-dynamic": "^7.2.1",
    "@angular/platform-server": "^7.2.1",
    "@angular/router": "^7.2.1",
    "@angular/service-worker": "^7.2.1",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "@ngx-translate/core": "^11.0.1",
    "angulartics2": "^7.4.1",
    "core-js": "^2.5.6",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.10",
    "material-design-icons-iconfont": "^4.0.4",
    "rxjs": "^6.3.1",
    "ts-loader": "^5.3.3",
    "tslib": "^1.9.0",
    "webpack-cli": "^3.2.1",
    "zone.js": "^0.8.28"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.3",
    "@angular/cli": "~7.2.2",
    "@angular/compiler-cli": "^7.2.1",
    "@angular/language-service": "^7.2.1",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^3.0.0",
    "@types/express": "^4.16.1",
    "@types/jasmine": "^3.3.7",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.108",
    "@types/node": "^10.12.18",
    "codelyzer": "^4.4.4",
    "express": "^4.16.4",
    "hads": "^1.7.0",
    "htmlhint": "^0.10.0",
    "https-proxy-agent": "^2.0.0",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.4.0",
    "reflect-metadata": "^0.1.13",
    "stylelint": "~9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.5.1",
    "ts-node": "~8.0.1",
    "tslint": "~5.12.1",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.2.4",
    "webpack-cli": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
